<!DOCTYPE html>
<html lang="en">
<head>
  <%- include('../layout/head') %>
  <link href="https://fonts.googleapis.com/css2?family=BioRhyme&display=swap" rel="stylesheet">
  <link rel="preconnect" href="https://fonts.gstatic.com">
  <link href="https://fonts.googleapis.com/css2?family=BioRhyme&family=MuseoModerno:wght@300&display=swap" rel="stylesheet">
</head>
<body>
  <header>
    <%- include('../layout/header') %>
  </header>
  <main>
    <div class="items">
      <h2> Education Opportunities </h2>
     
      <% if (school.length === 0) { %>
         <h4>"NO DATA AVAILABLE"</h4>
      <% } else { %>
        <% school.forEach( val => {  %>
      <div class="name-pic">
        <h3> <%= val.school_name %> </h3>
        <!-- <a href="<%= val.school_url %>"> <%= val.school_url %> </a> -->
        <!-- <a href="<%= val.school_url %> "> School Website </a> -->
      </div>
      <div class="description">
        <h4> <%= val.address %></h4>
        <h4> <%= val.city %>, <%= val.state_name %> </h4>
        <p> Program Name: <%= val.program_name %></p>
        <p> Program Length: <%= val.program_length %> </p>
        <a href="https://<%= val.school_url %> "> <%= val.school_url %> </a>
        <!-- <a href="/"> Expand </a> -->
        <p>~~</p>
      </div>

      <% }) %>
    <% } %>
    </div>

    <div class="meetup">
      <h2> Job Centers </h2>
      <% if (meetUp.length === 0) { %>
        <h4>"NO DATA AVAILABLE"</h4>
      <% } else { %>
      <% meetUp.forEach( val => { %>

        <div class="name-pic">
          <h3> <%= val.meet_up_name %> </h3>
        </div>
        <div class="description">
          <h4> <%= val.address %> </h4>
          <h6> <%= val.hours %> </h6>
          <p> <%= val.phone %> </p>
          <a href="<%= val.website %>"> Website </a>
          <!-- <a href="/"> Expand </a> -->
          <p> ~~</p>
        </div>
      <% }) %>
     <% } %>
    </div>
    
    
    <div class="jobs">
      <h2> Work Opportunities </h2>
<% if (job.length === 0) { %>
  <h4>"NO DATA AVAILABLE"</h4>
<% } else { %>
      <% job.forEach( val => { %>

        <div class="name-pic">
          <h3> <%= val.job_title %> </h3>
        </div>
        <div class="description">
          <h4> Company: <%= val.company %> </h4>
          <h6> <%= val.location %>  </h6>
          <p> val Posted: <%= val.date %>  </p>
          <a href="<%= val.url %>"> Company Site </a>
          <!-- <a href="/"> Expand </a> -->
          <p>~~</p>
        </div>

      <% }) %>
  <% } %>
    </div>
    
      
  </main>
  <footer>
    <%- include('../layout/footer') %>
  </footer>
</body>
</html>


 
